{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Curious Stats",
    "description": "Chrome extension that displays some interesting data about websites and what they know about you.",
    "background":{
        "persistent": false,
        "scripts": ["js/bgscript.js"]
    },
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
    }
}